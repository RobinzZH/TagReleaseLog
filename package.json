{
    "name": "ChangeLog",
    "version": "1.1.0",
    "description": "Tag release tool",
    "main": "index.js",
    "author": "RobinzZH",
    "scripts": {
        "lint": "eslint examples src test --fix",
        "precommit": "lint-staged",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "test": "mocha --recursive test",
        "coverage": "nyc --reporter=text npm test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RobinzZH/TagReleaseLog.git"
    },
    "devDependencies": {
        "@redshelf/git-release-notes": "github:RobinzZH/git-release-notes",
        "@commitlint/cli": "^6.2.0",
        "@commitlint/config-conventional": "^6.1.3",
        "chai": "^4.1.2",
        "eslint": "^4.10.0",
        "babel-eslint": "^8.2.3",
        "eslint-config-alloy": "^1.4.2",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.0",
        "mocha": "^5.0.5",
        "nyc": "^11.6.0",
        "sinon": "^4.1.2"
    },
    "engines": {
        "node": ">= 8.0.0"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "bin": {
        "changelog": "./index.js"
    },
    "nyc": {
        "include": [
            "src"
        ]
    }
}
